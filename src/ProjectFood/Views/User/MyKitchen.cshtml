@model ProjectFood.Models.ViewModels.UserVM.MyKitchenVM

@{
    ViewData["Title"] = "MyKitchen";
}
<style>
    body {
        padding-top: 130px;
    }
</style>


<input type='text' id='newFood' />

<input type='button' value='Lägg till matvara' id='addFood' />

<ul id='myKitchenList'></ul>
<br/>
<ul>
    @{foreach (var item in Model.MyFood)
        {
            <li>@item.FoodItem.Name</li>
        }
    }    
</ul>

